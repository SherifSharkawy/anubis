<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
android:id="@+id/sendmail"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TextView
android:id="@+id/tabcompose"
android:layout_width="158px"
android:layout_height="wrap_content"
android:background="#ff535252"
android:text="Compose"
android:textSize="16sp"
android:gravity="center"
android:layout_x="1px"
android:layout_y="0px"
>
</TextView>
<TextView
android:id="@+id/tabinbox"
android:layout_width="161px"
android:layout_height="wrap_content"
android:background="#ffcccccc"
android:text="Inbox"
android:textSize="16sp"
android:textStyle="bold"
android:textColor="#ff4f4e4e"
android:gravity="center"
android:layout_x="159px"
android:layout_y="0px"
>
</TextView>
<TextView
android:id="@+id/widget109"
android:layout_width="283px"
android:layout_height="wrap_content"
android:text="Coming Soon"
android:textSize="30sp"
android:typeface="monospace"
android:textStyle="bold"
android:textColor="#ff878686"
android:gravity="center"
android:layout_x="17px"
android:layout_y="179px"
>
</TextView>
</AbsoluteLayout>
